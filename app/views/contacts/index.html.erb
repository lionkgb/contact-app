
<table class="table table-bordered">
 <tr>
   <th>Full Name</th>
   <th>Email</th>
   <th>Phone number</th>
 </tr>
 <% @contacts.each do |contact| %>
   <tr>
     <td><a href="/contacts/<%= contact.id %>"><%= contact.full_name %></a></td>
     <td><%= contact.email %></td>
     <td><%= contact.prefix %></td>
   </tr>
 <% end %>

</table>